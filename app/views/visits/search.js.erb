<% if @logbooks.size > 0 %>
$('#not-found-result').hide();
$('#search-logbook-result').html('<%=j render(partial: "visits/logbook", collection: @logbooks) %>');
<% else %>
$('#search-logbook-result').html('');
$('#not-found-result').show();
<% end %>

onLoad(function(){
  new App.Utils.SelectsPickers();
});
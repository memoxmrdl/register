%h3 Buscar visitante
= form_tag search_logbook_path, remote: true, method: :get, id: 'search_logbook', class: 'form-inline' do
  .form-group
    = text_field_tag :search, params[:search], class: 'col-md-6 form-control'
  = submit_tag 'Buscar', class: 'btn btn-primary'
%br
%table.table.table-striped
  %thead
    %tr
      %th
      %th Nombre
      %th Apellidos
      %th
  %tbody#search-logbook-result
%p#not-found-result.text-center{style: 'display:none'} No se encuentro ningun resultado